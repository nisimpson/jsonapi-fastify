{
  "name": "jsonapi-fastify",
  "version": "0.0.0",
  "description": "A node JSON:API server, powered by fastify",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "precommit": "npm run test",
    "build": "npm run clean && tsc --project tsconfig.build.json && tsc-alias",
    "check": "tsc --noEmit",
    "clean": "rm -rf ./dist",
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nisimpson/jsonapi-fastify.git"
  },
  "keywords": [
    "jsonapi",
    "fastify",
    "api",
    "rest"
  ],
  "author": "Nathan Simpson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nisimpson/jsonapi-fastify/issues"
  },
  "homepage": "https://github.com/nisimpson/jsonapi-fastify#readme",
  "dependencies": {
    "@anatine/zod-openapi": "^1.3.0",
    "fastify": "^3.27.4",
    "openapi3-ts": "^2.0.1",
    "qs": "6.5.2",
    "zod": "^3.11.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/qs": "^6.9.7",
    "esbuild": "^0.14.28",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nanoid": "^3.3.1",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.6.6",
    "typescript": "^4.6.2"
  }
}
